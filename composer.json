{
    "name": "shopwarelabs/plugin-info",
    "description": "Read plugin metadata for shopware plugins",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-dom": "*",
        "ext-zip": "*",
        "justinrainbow/json-schema": "~5.2.10"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "kubawerlos/php-cs-fixer-custom-fixers": "^2.5.0",
        "phpunit/phpunit": "^9.5.4"
    },
    "autoload": {
        "psr-4": {
            "Shopware\\PluginInfo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shopware\\PluginInfo\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
